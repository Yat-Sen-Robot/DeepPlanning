{
  "Model": {
    "name": "vgg19v2",
    "backbone": "vgg19",
    "i_shape": [480, 480, 3],
    "A": 4,
    "B": 5,
    "S": 1,
    "w": 60.0,
    "h": 60.0,
    "tiny": false
  },

  "Train": {
    "batch": 16,
    "epoch": 2000,
    "frozen_epoch": 75,
    "ini_epoch": 0,
    "inputs_type": "rgous",
    "dataset": "../DataMaker/dataset",
    "training_set_size": 8221,
    "validation_set_size": 2741,
    "pretrained_weights_folder": "weights_of_pretrained_backbone",
    "load_pretrained_weights": true,
    "max_queue_size": 10,
    "summary": true,
    "verbose": 1
  },

  "Loss": {
    "type": "self",
    "coord": "bce",
    "lamb": 1e-4,
    "lam0": 1e4,
    "lam1": 1.0
  },

  "Optimizer": {
    "type": "adam",
    "lr": 3e-5,
    "beta_1": 0.9,
    "beta_2": 0.999,
    "momentum": 0.9,
    "decay": 0.0
  },

  "TensorBoard": {
    "enable": true,
    "write_graph": false
  },

  "LRSchedule": {
    "enable": true,
    "type": "steps",
    "wp_step": 0,
    "wp_lr": 0.0,
    "drop_epochs": [75, 105, 135],
    "drop_decay": 10.0
  },

  "CheckPoint": {
    "enable": true,
    "monitor": "loss",
    "only_weights": true,
    "period": 50
  },

  "EarlyStop": {
    "enable": false,
    "monitor": "loss",
    "patient": 100,
    "min_delta": 0.001,
    "verbose": 1,
    "mode": "auto"
  }
}
